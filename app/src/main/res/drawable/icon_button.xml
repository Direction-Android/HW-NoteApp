<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="oval">
            <solid android:color="#00FBFBFB" />
            <stroke
                android:width="3dp"
                android:color="#FDE401"
                />
            <padding
            android:left="1dp"
            android:top="1dp"
            android:right="1dp"
            android:bottom="1dp" />
            <corners android:radius="4dp" />
        </shape>
    </item>


</selector>